<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" id="WebApp_ID" version="6.0">
  <display-name>FoodAppWebApplication</display-name>
  <welcome-file-list>
  	<welcome-file>HomeServlet</welcome-file>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  
 
 
  
  
  <servlet>
    <servlet-name>RestaurantRegister</servlet-name>
    <servlet-class>com.foodapp.servlets.AddRestaurant</servlet-class>
</servlet>
 	<servlet-mapping>
 	<servlet-name>RestaurantRegister</servlet-name>
 	<url-pattern>/callingRestaurantRegisterServlet</url-pattern>
 	</servlet-mapping>
 
 
  <servlet>
    <servlet-name>MenuRegister</servlet-name>
    <servlet-class>com.foodapp.servlets.AddMenu</servlet-class>
</servlet>
 	<servlet-mapping>
 	<servlet-name>MenuRegister</servlet-name>
 	<url-pattern>/callingMenuServlet</url-pattern>
 	</servlet-mapping>
 	
 	
 	<servlet>
 		<servlet-name>GettingUser</servlet-name>
 		<servlet-class>com.foodapp.servlets.GettingUser</servlet-class>
 	</servlet>
 	<servlet-mapping>
 		<servlet-name>GettingUser</servlet-name>
 		<url-pattern>/GetUserServlet</url-pattern>
 	</servlet-mapping>
 	
 	<servlet>
 		<servlet-name>GetAllUsers</servlet-name>
 		<servlet-class>com.foodapp.servlets.GetAllUsers</servlet-class>
 	</servlet>
 	<servlet-mapping>
 		<servlet-name>GetAllUsers</servlet-name>
 		<url-pattern>/GetAllUsersServlet</url-pattern>
 	</servlet-mapping>
 	
 	<servlet>
 		<servlet-name>DeleteUser</servlet-name>
 		<servlet-class>com.foodapp.servlets.DeleteUser</servlet-class>
 	</servlet>
 	<servlet-mapping>
 		<servlet-name>DeleteUser</servlet-name>
 		<url-pattern>/DeleteUserServlet</url-pattern>
 	</servlet-mapping>
 	
 	
 	<servlet>
 		<servlet-name>UpdateUser</servlet-name>
 		<servlet-class>com.foodapp.servlets.UpdateUser</servlet-class>
 	</servlet>
 	<servlet-mapping>
 		<servlet-name>UpdateUser</servlet-name>
 		<url-pattern>/UpdateUserServlet</url-pattern>
 	</servlet-mapping>
 	
 	

 </web-app>